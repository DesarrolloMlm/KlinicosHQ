@model EstablecimientoPanelDeControl.ViewModels.ProblemaSaludViewModel
@{
    ViewBag.Title = "_ProblemaSalud";
}

<table class="table table-hover table-striped table-bordered">
    <thead>
        <tr>
            <th>Problema de Salud</th>
            <th>Cantidad</th>
        </tr>
    </thead>
    <tbody>

        @for (int i = 0; i < Model.problemasSaludListado.Count(); i++)
        {
            <tr>
                <td>@Model.problemasSaludListado[i].problemaSalud</td>
                <td>@Model.problemasSaludListado[i].cantidad</td>
            </tr>
        }
    </tbody>


</table>